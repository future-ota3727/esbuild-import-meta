{
  "name": "esbuild-import-meta",
  "version": "1.0.0",
  "type": "module",
  "private": true,
  "scripts": {
    "esm": "esbuild test.js --bundle --outdir=dist --platform=node --format=esm && node dist/test.js",
    "cjs": "esbuild test.js --bundle --outdir=dist --platform=node --format=cjs --out-extension:.js=.cjs && node dist/test.cjs"
  },
  "dependencies": {
    "@kintone/rest-api-client": "5.0.8",
    "esbuild": "0.20.0"
  },
  "volta": {
    "node": "18.18.0",
    "npm": "9.8.1"
  }
}
